Algoritmo "0001 - Prática Procedimento"

Var

  procedimento usuario
  var
  contador : inteiro
  user : caracter
  inicio
    contador := 0
    para contador de 1 ate 10 faca
      leia(user)
      limpatela
      escreval("É um prazer te conhecer ", user, "!")
    fimpara
  fimprocedimento

Inicio
  usuario


Fimalgoritmo